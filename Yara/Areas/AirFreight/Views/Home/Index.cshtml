@model ViewmMODeElMASTER

@{
    ViewData["Title"] = "Index";
    Layout = "~/Areas/AirFreight/Views/Shared/_LayoutAirFreight.cshtml";
}
<link rel="stylesheet" href="~/css/home.css">

<div class="row small-spacing">
    <!-- /.col-lg-3 col-md-6 col-xs-12 -->
    <div class="col-lg-6 col-md-6 col-xs-12">
        <div class="box-content">

            <div class="content widget-stat-chart">
                <div class="c100 p76 small blue js__circle">
                    <span>76%</span>
                    <div class="slice">
                        <div class="bar"></div>
                        <div class="fill"></div>
                    </div>
                </div>
                <!-- /.c100 p58 -->
                <div class="right-content">
                    <h4 class="box-title">Cost Price</h4>
                    <h2 class="counter" style="color:green">@ViewBag.Favorit</h2>
                    <h4 class="box-title">	price</h4>
                    <h2 class="counter" style="color:darkred">@ViewBag.price</h2>
                    <h4 class="box-title">@ResourceWeb.LbExchangedPrice</h4>
                    <h2 class="counter" style="color:blueviolet">@ViewBag.ExchangedPrice</h2>
                    <h4 class="box-title">tottal</h4>
                    <h2 class="counter" style="color:aqua">@ViewBag.total</h2>
                    <h4 class="box-title">paidings</h4>
                    <h2 class="counter" style="color:chocolate">@ViewBag.paidings</h2>
                    <h4 class="box-title">Transfer</h4>
                    <h2 class="counter" style="color:darkgreen">@ViewBag.Trans</h2>

                    <!-- /.counter -->
                    <!-- /.text -->
                </div>
                <!-- /.right-content -->
            </div>
            <!-- /.content -->
        </div>
        <!-- /.box-content -->
    </div>
