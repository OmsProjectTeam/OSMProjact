@model ViewmMODeElMASTER
@{
    ViewData["Title"] = ResourceWebAr.LBAddShippingPrice;
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}



<div class="col-xs-12">
    <div class="box-content">
        <ul class="list-inline">
            <li class="margin-bottom-10"><a href="#"><button type="button" class="btn btn-primary btn-rounded btn-bordered waves-effect waves-light">@ResourceWebAr.LBAddShippingPrice</button></a></li>/
            <li class="margin-bottom-10"><a asp-area="Admin" asp-controller="Home" asp-action="Index"><button type="button" class="btn btn-primary btn-rounded btn-bordered waves-effect waves-light">Admin Home</button></a></li>/
            <li class="margin-bottom-10"><a asp-area="" asp-controller="Home" asp-action="Index"><button type="button" class="btn btn-primary btn-rounded btn-bordered waves-effect waves-light">Home</button></a></li>
            <span style="color:royalblue"> <strong>@TempData["Saved successfullyl"]</strong></span>
            <span style="color:red"> <strong>@TempData["ErrorSave"]</strong></span>


        </ul>
        <div class="row small-spacing">
            <div class="col-lg-6 col-xs-12">
                <div class="box-content card white">
                    <h4 class="box-title">@ResourceWebAr.LBAddShippingPrice</h4>
                    <!-- /.box-title -->



                    <div class="card-content">
                        <form role="form" method="post" asp-controller="ShippingPrice" asp-action="SaveAr" enctype="multipart/form-data">
                            <div class="form-group">
                                <input type="hidden" asp-for="@Model.ShippingPrice.DataEntry" class="form-control" value="@User.Identity.Name">
                                <input type="hidden" asp-for="@Model.ShippingPrice.IdShipping" class="form-control">
                                <input type="hidden" asp-for="@Model.ShippingPrice.DateTimeEntry" value="@DateTime.Now">
                                <input type="hidden" asp-for="@Model.returnUrl" id="url">

                            </div>

                            <div class="card-content">
                                <div class="row small-spacing">
                                    <div class="col-12 col-sm-4">
                                        <div class="form-group ">
                                            <label asp-for="@Model.ShippingPrice.IdTypeSystem" for="exampleInputEmail1">@ResourceWebAr.LbTypeSystem</label>
                                            <select class="form-control" asp-for="@Model.ShippingPrice.IdTypeSystem" asp-items="@(new SelectList(ViewBag.TypeSystem,"IdTypeSystem","TypeSystem"))">
                                            </select>
                                            <span asp-validation-for="@Model.ShippingPrice.IdTypeSystem" class="text-danger"></span>
                                        </div>
                                    </div>
                                    <div class="col-12 col-sm-4">
                                        <div class="form-group ">
                                            <label asp-for="@Model.ShippingPrice.IdCurrenciesExchangeRates" for="exampleInputEmail1">@ResourceWebAr.LbCountry</label>
                                            <select class="form-control" asp-for="@Model.ShippingPrice.IdCurrenciesExchangeRates" asp-items="@(new SelectList(ViewBag.Currenc,"IdCurrenciesExchangeRates","Country"))">
                                            </select>
                                            <span asp-validation-for="@Model.ShippingPrice.IdCurrenciesExchangeRates" class="text-danger"></span>
                                        </div>
                                    </div>
                                    <div class="col-12 col-sm-4">
                                        <div class="form-group ">
                                            <label asp-for="@Model.ShippingPrice.IdInformationCompanies" for="exampleInputEmail1">@ResourceWebAr.LbTypeSystem</label>
                                            <select class="form-control" asp-for="@Model.ShippingPrice.IdInformationCompanies" asp-items="@(new SelectList(ViewBag.InformationCompanies,"IdInformationCompanies","CompanyName"))">
                                            </select>
                                            <span asp-validation-for="@Model.ShippingPrice.IdInformationCompanies" class="text-danger"></span>
                                        </div>
                                    </div>



                                </div>

                            </div>
                            <div class="card-content">
                                <div class="row small-spacing">
                                    <div class="col-12 col-sm-4">
                                        <div class="form-group ">
                                            <label asp-for="@Model.ShippingPrice.TitleShipping" for="exampleInputEmail1">@ResourceWebAr.LTitleShipping</label>
                                            <input asp-for="@Model.ShippingPrice.TitleShipping" type="text" placeholder="@ResourceWebAr.LTitleShipping ...." class="form-control" maxlength="300">
                                            <span asp-validation-for="@Model.ShippingPrice.TitleShipping" class="text-danger"></span>
                                            <span style="color:red"> <strong>@TempData["Country"]</strong></span>
                                        </div>
                                    </div>
                                    <div class="col-12 col-sm-4">
                                        <div class="form-group ">
                                            <label asp-for="@Model.ShippingPrice.CoPricePerkgUnder10" for="exampleInputEmail1">@ResourceWebAr.LbCoPricePerkgUnder10</label>
                                            <input asp-for="@Model.ShippingPrice.CoPricePerkgUnder10" type="text" placeholder="@ResourceWebAr.LbCoPricePerkgUnder10 ...." class="form-control" maxlength="20">
                                            <span asp-validation-for="@Model.ShippingPrice.CoPricePerkgUnder10" class="text-danger"></span>

                                        </div>
                                    </div>

                                    <div class="col-12 col-sm-4">
                                        <div class="form-group ">
                                            <label asp-for="@Model.ShippingPrice.CoPricePerkgAbove10" for="exampleInputEmail1">@ResourceWebAr.LbCoPricePerkgAbove10</label>
                                            <input asp-for="@Model.ShippingPrice.CoPricePerkgAbove10" type="text" placeholder="@ResourceWebAr.LbCoPricePerkgAbove10 ...." class="form-control" maxlength="20">
                                            <span asp-validation-for="@Model.ShippingPrice.CoPricePerkgAbove10" class="text-danger"></span>

                                        </div>
                                    </div>
                                </div>

                            </div>

                            <div class="card-content">
                                <div class="row small-spacing">
                                    <div class="col-12 col-sm-3">
                                        <div class="form-group ">
                                            <label asp-for="@Model.ShippingPrice.ClintPricePerkgUnder10" for="exampleInputEmail1">@ResourceWebAr.LbClintPricePerkgUnder10</label>
                                            <input asp-for="@Model.ShippingPrice.ClintPricePerkgUnder10" type="text" placeholder="@ResourceWebAr.LbClintPricePerkgUnder10 ...." class="form-control" maxlength="20">
                                            <span asp-validation-for="@Model.ShippingPrice.ClintPricePerkgUnder10" class="text-danger"></span>
                                        </div>
                                    </div>
                                    <div class="col-12 col-sm-3">
                                        <div class="form-group ">
                                            <label asp-for="@Model.ShippingPrice.ClintPricePerkgAbove10" for="exampleInputEmail1">@ResourceWebAr.LbClintPricePerkgAbove10</label>
                                            <input asp-for="@Model.ShippingPrice.ClintPricePerkgAbove10" type="text" placeholder="@ResourceWebAr.LbClintPricePerkgAbove10 ...." class="form-control" maxlength="20">
                                            <span asp-validation-for="@Model.ShippingPrice.ClintPricePerkgAbove10" class="text-danger"></span>

                                        </div>
                                    </div>
                                    <div class="col-12 col-sm-3">
                                        <div class="form-group ">
                                            <div class="checkbox margin-bottom-20">
                                                <div class="switch success"><input type="checkbox" asp-for="@Model.ShippingPrice.Active" id="switch-2" checked="checked"><label for="switch-2">@ResourceWebAr.LbActive</label></div>
                                            </div>

                                        </div>
                                    </div>

                                </div>

                            </div>







                            <div class="checkbox margin-bottom-20">
                                <div class="switch success"><input type="checkbox" asp-for="@Model.ShippingPrice.CurrentState" id="switch-10" checked="checked"><label for="switch-10">@ResourceWebAr.lbCurrentState</label></div>
                            </div>
                            <button type="submit" class="btn btn-primary btn-sm waves-effect waves-light">@ResourceWebAr.lbSave</button>
                        </form>
                    </div>
                    <!-- /.card-content -->
                </div>
                <!-- /.box-content -->
            </div>


            <div class="col-lg-6 col-xs-12">
                <div class="box-content card white">
                    <h4 class="box-title">@ResourceWebAr.lbOldeAndUserGide</h4>
                    <!-- /.box-title -->
                    <div class="card-content">

                        @*   <div class="form-group">
                        <img src="~/images/user gide/Product.png" width="1920" height="1077" />
                        </div> *@


                    </div>
                    <!-- /.card-content -->
                </div>
                <!-- /.box-content -->
            </div>
        </div>
        <!-- /.row -->
    </div>
    <!-- /.box-content -->
</div>


@section Scripts {
    <script>
        var loadFile = function (event) {
            var image = document.getElementById('output');
            image.src = URL.createObjectURL(event.target.files[0]);
        };
    </script>
    <script>
        $("#url").val(window.location.href);
    </script>
}

